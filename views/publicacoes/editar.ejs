<%-include('../header')%>
        <h1>Editar Publicação</h1>
        <form action="/publicacoes/editar/<%=publicacoes.id%>/<%=categoria%>" method="post">
            <div class="form-group">
                <label for="titulo">Titulo</label>'
                <input type="text" required name="titulo" value="<%=publicacoes.titulo%>" id="titulo" class="form-control" placeholder="Digite o titulo...">
            </div>
           
            <div class="form-group">
                    <label for="publicacao">Nova Publicação</label>
                    <textarea class="form-control" name="conteudo" required id="" cols="30" rows="10" placeholder="Digite o conteudo"><%=publicacoes.conteudo%></textarea>
            </div>
            <div class="form-group">
                <input type="submit" value="Salvar" class="btn btn-success">
            </div>
        </form>
 <%-include('../footer')%>